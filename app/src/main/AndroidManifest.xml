<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.itfitness.customdeom">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!--三角路径-->
        <activity android:name=".activitys.TrianglePathActivity" android:launchMode="singleTop" android:screenOrientation="portrait"/>
        <!--扫描动画-->
        <activity android:name=".activitys.ScanAnimActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <!--蛛网控件-->
        <activity android:name=".activitys.CobWebActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <!--改变颜色-->
        <activity android:name=".activitys.ChangeColorActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <!--加载动画-->
        <activity android:name=".activitys.LoadingViewActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
        <!--菜单动画-->
        <activity android:name=".activitys.AnimMenuActivity" android:screenOrientation="portrait" android:launchMode="singleTop"/>
    </application>

</manifest>